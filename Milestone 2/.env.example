# Environment Variables Template for Real Estate AI Application
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Google Gemini AI API Key (Required)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration (Optional - will use CSV fallback if not configured)
DATABASE_URL=postgresql://username:password@localhost:5432/realestate_db

# Application Settings
DEBUG=True
SECRET_KEY=your-secret-key-here

# Logging Level
LOG_LEVEL=INFO
